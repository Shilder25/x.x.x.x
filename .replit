modules = ["python-3.11", "postgresql-16", "nodejs-20"]

[nix]
channel = "stable-25_05"
packages = ["glibcLocales"]

[deployment]
deploymentTarget = "autoscale"
run = ["sh", "-c", "python app.py"]

[agent]
integrations = ["python_xai:1.0.0", "python_gemini_ai_integrations:1.0.0", "python_openai_ai_integrations:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "main"

[[workflows.workflow]]
name = "main"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "console"

