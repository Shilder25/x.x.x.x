modules = ["python-3.11"]

[nix]
channel = "stable-25_05"
packages = ["glibcLocales"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[agent]
integrations = ["python_xai:1.0.0", "python_gemini_ai_integrations:1.0.0", "python_openai_ai_integrations:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "public_dashboard"

[[workflows.workflow]]
name = "public_dashboard"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run public_dashboard.py --server.port 5000"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 36877
externalPort = 3002

[[ports]]
localPort = 44275
externalPort = 3000

[[ports]]
localPort = 45103
externalPort = 3001
